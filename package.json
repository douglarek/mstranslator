{
    "name": "translator",
    "displayName": "Translator",
    "description": "A translator using Youdao Translate",
    "version": "0.4.2",
    "publisher": "douglarek",
    "repository": {
        "url": "https://github.com/douglarek/translator"
    },
    "license": "MIT",
    "icon": "images/icon.png",
    "engines": {
        "vscode": "^1.14.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:translator.translate"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [
            {
                "command": "translator.translate",
                "title": "Translator: Translate"
            }
        ],
        "keybindings": [
            {
                "command": "translator.translate",
                "key": "alt+1",
                "when": "editorTextFocus"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "dependencies": {
        "request": "^2.81.0",
        "request-promise": "^4.2.1",
        "@types/request-promise": "^4.1.36"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    }
}